{
    "configurations": [
        {
            "name": "AgosGE_vscode_config",
            "includePath": [
                "${workspaceFolder}/",
                "${workspaceFolder}/**",
                "${workspaceFolder}/Agos/",
                "${workspaceFolder}/Agos/**",
                "${workspaceFolder}/Agos/vendor/EventBus/lib/src"
            ],
            "defines": [
                "AG_SHADERS_PATH=\"\"",
                "AG_MODELS_PATH=\"\""
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerArgs": [
                "-Wall",
                "-Wextra",
                "-std=c++17"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/",
                    "${workspaceFolder}/**",
                    "${workspaceFolder}/Agos/"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}